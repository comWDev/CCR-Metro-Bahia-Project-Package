@inherits Umbraco.Web.Mvc.UmbracoTemplatePage<ContentModels.Master>
@using ContentModels = Umbraco.Web.PublishedContentModels;
@{
	Layout = null;
}

<!DOCTYPE html>
<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->
    <title>@Umbraco.Field("pageName") | CCR Metrô Bahia</title>

    <!-- Bootstrap -->
    <link rel="stylesheet" type="text/css" href="~/css/sample_styles.css"/>    
    <link rel="stylesheet" type="text/css" href="~/css/bootstrap.css">
    <link rel="stylesheet" type="text/css" href="~/css/main.css">

    <!-- slick carosel -->
    <link rel="stylesheet" type="text/css" href="~/slick/slick.css"/>
    <!-- slick-theme.css if you want the default styling -->
    <link rel="stylesheet" type="text/css" href="~/slick/slickcss/slick-theme.css"/>

    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
